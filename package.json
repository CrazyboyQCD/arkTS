{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.17.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/volarjs/starter.git"
  },
  "scripts": {
    "build": "cd ./packages/vscode && npm run build",
    "watch": "cd ./packages/vscode && npm run watch",
    "pack": "cd ./packages/vscode && npm run pack",
    "generate": "cd ./packages/vscode && npm run update",
    "release": "pnpm -F \"{packages/*}\" build && tsx ./scripts/publish.ts",
    "test": "vitest --coverage --ui",
    "lint": "eslint && oxlint && tsc --build --noEmit && vitest --coverage",
    "prepare": "pnpm -F \"{packages/*}\" build"
  },
